{
	"name": "agnostic-devkit",
	"version": "1.4.0-canary.2",
	"description": "A browser extension built with Vite, TypeScript, SolidJS, and Tailwind CSS v4",
	"type": "module",
	"scripts": {
		"dev": "wxt",
		"watch": "wxt",
		"build": "wxt build",
		"build:chrome": "wxt build -b chrome",
		"build:firefox": "wxt build -b firefox",
		"build:all": "yarn build:chrome && yarn build:firefox",
		"zip": "wxt zip",
		"zip:chrome": "wxt zip -b chrome",
		"zip:firefox": "wxt zip -b firefox",
		"zip:all": "yarn zip:chrome && yarn zip:firefox",
		"postinstall": "wxt prepare",
		"build:coverage": "CI_COVERAGE=true yarn build",
		"type-check": "tsc --noEmit",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"check": "yarn lint && yarn format:check",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage && yarn coverage:score:vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:coverage": "CI_COVERAGE=true playwright test && yarn coverage:playwright && yarn coverage:score:playwright",
		"test:all:coverage": "rm -rf coverage/* && yarn test:coverage && yarn build:coverage && yarn test:e2e:coverage && yarn coverage:merge && yarn coverage:badges",
		"coverage:merge": "node scripts/merge-coverage.js",
		"coverage:playwright": "node scripts/generate-playwright-coverage.js",
		"coverage:score:vitest": "node scripts/get-coverage-score.js vitest",
		"coverage:score:playwright": "node scripts/get-coverage-score.js playwright",
		"coverage:badges": "node scripts/generate-coverage-badges.js",
		"coverage:open:vitest": "npx --yes open-cli coverage/vitest/index.html",
		"coverage:open:playwright": "npx --yes open-cli coverage/playwright/index.html",
		"check:unused": "node scripts/check-unused.js",
		"package": "yarn zip:all",
		"release:dry-run": "semantic-release --dry-run --no-ci"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"lz-string": "^1.5.0",
		"solid-js": "^1.9.10",
		"tailwind-merge": "^3.4.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.57.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/exec": "^7.1.0",
		"@semantic-release/git": "^10.0.1",
		"@tailwindcss/vite": "^4.1.17",
		"@trivago/prettier-plugin-sort-imports": "^6.0.2",
		"@types/archiver": "^7",
		"@types/eslint-plugin-jsx-a11y": "^6",
		"@types/jsdom": "^27",
		"@types/lz-string": "^1.5.0",
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/ui": "^4.0.16",
		"@wxt-dev/module-solid": "^1.1.4",
		"archiver": "^7.0.1",
		"baseline-browser-mapping": "^2.9.11",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-better-tailwindcss": "^4.0.2",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-solid": "^0.14.5",
		"globals": "^17.1.0",
		"happy-dom": "^20.0.10",
		"jsdom": "^27.2.0",
		"knip": "^5.70.2",
		"monocart-coverage-reports": "^2.12.9",
		"playwright": "^1.57.0",
		"prettier": "^3.8.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"semantic-release": "^25.0.2",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.0",
		"vite-plugin-istanbul": "^7.2.1",
		"vite-plugin-scope-tailwind": "^2.0.2",
		"vitest": "^4.0.16",
		"wxt": "^0.20.13"
	},
	"packageManager": "yarn@4.10.3+sha512.c38cafb5c7bb273f3926d04e55e1d8c9dfa7d9c3ea1f36a4868fa028b9e5f72298f0b7f401ad5eb921749eb012eb1c3bb74bf7503df3ee43fd600d14a018266f",
	"volta": {
		"node": "25.5.0",
		"yarn": "4.12.0"
	}
}
